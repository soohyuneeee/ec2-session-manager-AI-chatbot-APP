{
  "name": "ec2-session-manager-web",
  "version": "1.0.0",
  "description": "Web-based EC2 Session Manager with AI Chatbot",
  "main": "server/index.js",
  "scripts": {
    "dev": "npm run redis:start && concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "redis:start": "docker start ec2-session-redis 2>/dev/null || docker-compose up -d redis",
    "redis:stop": "docker-compose stop redis",
    "redis:logs": "docker logs ec2-session-redis",
    "redis:cli": "docker exec -it ec2-session-redis redis-cli"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.958.0",
    "@aws-sdk/client-ec2": "^3.958.0",
    "@aws-sdk/client-iam": "^3.958.0",
    "@aws-sdk/client-ssm": "^3.958.0",
    "@aws-sdk/client-sts": "^3.958.0",
    "@aws-sdk/credential-providers": "^3.958.0",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-pty": "^1.0.0",
    "redis": "^4.6.0",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
